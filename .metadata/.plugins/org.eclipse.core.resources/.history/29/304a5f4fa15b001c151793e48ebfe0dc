
public class Addition 
	{
	
		public static int Add(int a, int c) 
		{
			int b = a + c;
			return b;
		}

	public static void main(String[] args) 
	{
		//calling of the method
		int result = Add(45, 100);
		System.out.println("The result of the addition of the two numbers supplied by hard codiing is " + result);

	}

}
